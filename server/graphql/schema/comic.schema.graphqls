directive @inherits(type: String!) on OBJECT

type CreateComicInput {
  name: String!
  description: String
}
type CreateComicInputModel @inherits(type: "CreateComicInput") {
  CreatedBy: ID!
}

type Comic @inherits(type: "CreateComicInputModel") {
  _id: ID!
  createdAt: Time!
  updatedAt: Time!
}
